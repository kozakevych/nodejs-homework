{
  "name": "nodejs-homework",
  "version": "1.0.0",
  "description": "hometasks for nodejs program",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node ./app/index.ts",
    "express-6": "ts-node ./6-express/index.ts",
    "nosql-7": "ts-node ./app/index.ts",
    "auth-8": "ts-node ./app/index.ts",
    "tools-10": "ts-node ./app/index.ts",
    "tools-10:prod": "NODE_ENV=production ts-node ./app/index.ts",
    "seeds": "node ./app/seeds/seed-products.js && node ./app/seeds/seed-carts.js",
    "build": "tsc",
    "lint": "eslint .",
    "security-check": "npm audit --json"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm test",
      "post-merge": "npm run security-check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kozakevych/nodejs-homework.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kozakevych/nodejs-homework/issues"
  },
  "homepage": "https://github.com/kozakevych/nodejs-homework#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/debug": "^4.1.9",
    "@types/express": "^4.17.19",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@inquirer/prompts": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.10.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.1.0",
    "mongoose": "^7.6.1",
    "winston": "^3.11.0"
  }
}
